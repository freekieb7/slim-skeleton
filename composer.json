{
  "name": "freekieb7/slim-skeleton",
  "description": "Slim 4 skeleton",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "freekieb7"
    }
  ],
  "require": {
    "php": "^8.3",
    "slim/psr7": "^1.6",
    "slim/slim": "4.*",
    "php-di/slim-bridge": "^3.4"
  },
  "require-dev": {
    "ext-xdebug": "^3.0",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
    },
    "process-timeout": 0,
    "sort-packages": true
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --configuration tests/phpunit.xml"
  }
}
